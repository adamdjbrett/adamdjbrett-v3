<section class="cv-section">
<header class="cv-section-header">
<h2 class="cv-title-work"><span><strong>
{{experience.title}}
</strong></span></h2>
</header>
<div class="cv-item">
{% for expr in experience.text %}
<span class="cv-basic-info-item"><strong>{{ expr.title | safe }}</strong></span>
{% for xsp in expr.list %}
<div class="cv-item-content p-3 ms-5 lim"><ul><li>{{ xsp | safe }}</li></ul></div>
{% endfor %}
{% endfor %}
</div>
</section>